# Copy this file to `.env` and fill in the values based on your deployment.

# Public key of the Cashu relay/server Cashubash should connect to.
# Example: SERVER_PUBKEY=aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa
SERVER_PUBKEY=

# Default mint base URL used when a mint URL is not provided explicitly.
# Leave blank to fall back to https://mint.minibits.cash/Bitcoin.
# Example: MINT_URL=https://mint.minibits.cash/Bitcoin
MINT_URL=
